<!-- Modal -->
<div class="modal" id="tweet-modal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="btn-close mx-0" data-bs-dismiss="modal" aria-label="Close" id="close-modal-btn"></button>
        <h1 class="modal-title fs-5" id="exampleModalLabel">Drafts</h1>
      </div>
      <div class="modal-body">
        <%= render "shared/tweet_form", button_label: "Post", form_submit_path: tweets_path %>
      </div>
    </div>
  </div>
</div>
