<div class="col-7 border-start border-end">
  <div class="mb-3">
    <%= link_to "Home", dashboard_path, class: "fs-4 text-decoration-none text-black fw-semibold" %>
    <div class="row text-muted text-center mt-4 border-bottom py-3">
      <div class="col">
        For you
      </div>
      <div class="col">
        Following
      </div>
    </div>
  </div>
  <div class="d-flex py-1 px-3 mb-2 border-bottom">
    <div class="flex-shrink-0">
      <%= turbo_frame_tag dom_id(current_user) do %>
        <%= image_tag current_user.avatar, size: "48x48", class: "rounded-circle" %>
      <% end %>
    </div>
    <div class="flex-grow-1 ms-3 ">
      <%= render "shared/tweet_form" %>
    </div>
  </div>
  <div id="tweets">
    <%= render partial: "tweets/tweet", collection: @tweet, as: :tweet_presenter %>
  </div>
  <%# < Tweet.all.reverse.each do |tweet| >
    <= render "tweets/tweet", tweet: tweet >
  < end > %>
</div>
<div class="col">
column
</div>
